{
    "name": "flatgreen/waux",
    "description": "Try to extract media (audio) information from the web",
    "keywords": ["scraping, audio"],
    "type": "library",
    "require": {
        "php": ">=8.1",
        "symfony/event-dispatcher": "^6.4|^7.0",
        "symfony/cache": "^6.4|^7.0",
        "symfony/dom-crawler": "^6.4|^7.0",
        "symfony/css-selector": "^6.4|^7.0",
        "flatgreen/rfrance": "^2.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flatgreen\\WebMediaExtract\\": "src/",
            "Layered\\PageMeta\\": "src/"
        },
        "files": [
            "src/utils.php"
        ]
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "symfony/var-dumper": "^7.2"
    },
    "scripts": {
        "phpstan": "phpstan analyse --level 7 src/"
    }
}
